{
    "name": "epignosis/assignment",
    "description": "PHP Developer assignment",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "require": {
        "symfony/var-dumper": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "friendsofphp/php-cs-fixer": "^3.86"
    },
    "scripts": {
        "test":"@php vendor/bin/phpunit",
        "fix": "vendor/bin/php-cs-fixer fix",
        "fix:dry": "vendor/bin/php-cs-fixer fix --dry-run --diff"
    }
}
